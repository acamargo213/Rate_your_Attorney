<html>
    <head>
        <title>Rate your Attorney</title>
    </head>
    <body>
        <center>
          <%= form_tag('/law_offices', method: :get) do %>
            <h1 style= "font-size:49px; margin-top:260px">Rate your Attorney</h1>
        
            <%= text_field_tag :search, nil, size:70, placeholder: "Enter Law Office name, city, or state" %>
            <input type= "submit" value= "Go"/>
            <% end %>
        </center>
    </body>
</html>